type Mutation {
    CreateSong(song:SongInput) : Song
}
input SongInput {
    id:String!
    score:Float!
    name: String!
    number:Int
    album: AlbumInput
    artist:ArtistInput
}
input ArtistInput {
    id: String!
    name:String!
    thumbnail:String
}
input AlbumInput {
    id: String!
    name: String!
    thumbnail:String
    year: Int
}
