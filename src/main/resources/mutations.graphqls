type Mutation {
    CreateAlbum(album:NewAlbumInput): Album
    UpdateSong(song:SongInput) : Song
    DeleteSong(songId:String!) : Boolean
    DeleteAlbum(albumId:String!): Boolean
}

input SongInput {
    id:String!
    score:Float
}

input NewAlbumInput {
    vendorId: String
    name: String!
    thumbnail:String
    year: Int
    artist:ArtistInput
    songs:[NewSongInput!]
}


input NewSongInput {
    vendorId:String
    score:Float
    name: String!
    number:Int
    discNumber:Int
}
input ArtistInput {
    vendorId: String
    name:String!
    thumbnail:String
}
